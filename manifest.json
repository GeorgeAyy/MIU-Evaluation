{
  "manifest_version": 3,
  "name": "MIU Auto Evaluation",
  "version": "1.0",
  "permissions": ["activeTab", "storage"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": "Logo.png"
  },
  "content_scripts": [
    {
      "matches": ["https://portal.miuegypt.edu.eg/*"],
      "js": ["content.js"]
    }
  ]
}
